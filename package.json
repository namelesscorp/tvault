  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "tauri": "tauri",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,json}\" && yarn lint:eslint --fix",
    "lint:eslint": "eslint src/**/*.{ts,tsx,js,json}",
    "tauri:dev": "yarn tauri dev",
